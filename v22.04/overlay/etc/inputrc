"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[7~": beginning-of-line
"\e[8~": end-of-line
"\eOH": beginning-of-line
"\eOF": end-of-line
"\e[H": beginning-of-line
"\e[F": end-of-line

"\e[1;5C": forward-word
"\e[1;5D": backward-word
"\eOC": forward-word
"\eOD": backward-word
"\eOc": forward-word
"\eOd": backward-word
"\e\e[C": forward-word
"\e\e[D": backward-word

"\e[5~": history-search-backward
"\e[6~": history-search-forward
